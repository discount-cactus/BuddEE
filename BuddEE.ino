      wave = pgm_read_byte_near(sine20000 + sinNum);
      break;
  }

  PORTL = wave; //pins 42-49

  PWCurrent = analogRead(potPin_PWM);
  if (abs(PWCurrent-pulseWidth) > PWTolerance){ //if reading from pot exceeds tolerance
    pulseWidth = PWCurrent;                     //new pulse width, val between 0 and 1023
    pulseWidthScaled = int(pulseWidth/1023*period);
  }

  //Prints the input parameters of the generator in the serial monitor
  Serial.print("Shape: ");
  switch(shape){
    case 0:
      Serial.println("Pulse");
      break;
    case 1:
      Serial.println("Triangle");
      break;
    case 2:
      Serial.println("Saw");
      break;
    case 3:
      Serial.println("Sine");
      break;
  }
  Serial.println("Frequency: " + String(frequency) + "Hz");
  Serial.println("Pulse Width: " + String(pulseWidth));
  Serial.println("");
}


void oscilloscope(){
  int OSC_val = analogRead(OSCpin);

  Serial.write(0xff);
  Serial.write((OSC_val >> 8) & 0xff);
  Serial.write(OSC_val & 0xff);
}



void setup() {
  Serial.begin(9600);
  //set port/pin  mode
  DDRD = 0xFF;    //all outputs
  DDRC = 0x00;    //all inputs
  DDRB = 0xFF;    //all outputs
  //TIMER INTERRUPT SETUP
  
  cli();          //disable interrupts
  //timer 1:
  //TCCR1A = 0;     // set entire TCCR1A register to 0
  //TCCR1B = 0;     // same for TCCR1B
  
  OCR1A = 159;              //set compare match register- 100khz to start,  = (16 000 000 / 100 000) - 1 = 159
  //TCCR1B |= (1 << WGM12);   //turn on CTC mode
  //TCCR1B |= (1 << CS10);    // Set CS10 bit for 0 prescaler
  //TIMSK1 |= (1 << OCIE1A);  // enable timer compare interrupt
  
  samplerate = 100000;
  
  //initialize variables
  frequency = analogRead(potPin_frequency);     //initialize frequency
  freqscaled = 48*frequency+1;    //from 1 to ~50,000 Hz
  period = samplerate/freqscaled;
   
  pulseWidth = analogRead(potPin_PWM);    //initalize pulse width
  pulseWidthScaled = int(pulseWidth/1023*period);
  
  triInc = 511/period;
  sawInc = 255/period;
  sinInc = 20000/period;
  
  sei();                    //enable interrupts
}

