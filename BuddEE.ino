void calculate_voltage() { 
  float R1 = 1000000.00; 
  float R2 = 100000.00; 
  float v_ref = 5.00; 
  float resistor_ratio = (R2 / (R1 + R2)); 
  float adc_value = 0.00; 
  float v = 0.00;
  for (int i = 0; i < 20 ; i++) 
  { 
    adc_value = adc_value + analogRead(Voltmeter);
    delay(3); 
  } 
  adc_value = adc_value / 20; 
  v = ((adc_value * v_ref) / 1024); 
  voltage = v / resistor_ratio;

  Serial.print(voltage); 
  Serial.println(" v");

  delay(500);
}

void calculate_current() { 
  int sensitivity = 185; 
  int adc_value = 0; 
  float v_ref = 4.94; 
  float voltage = 0.00; 
  float pure_voltage = 0.00; 
  float offset_voltage = 2.47; 
  for (int i = 0; i < 40 ; i++) 
  { 
    adc_value = adc_value + analogRead(Ammeter); 
    delay(2); 
  } 
  adc_value = adc_value / 40; 
  voltage = ((adc_value * v_ref) / 1024); 
  pure_voltage = voltage - offset_voltage; 
 // if(pure_voltage > 0.001) pure_voltage = 0.00; 
  pure_voltage = pure_voltage * 1000; 
  current = pure_voltage / sensitivity;
  Serial.println(String("I = ") + current + "A");

  delay(500);
}

void calculate_resistance() {
  const int R1 = 4;
  const int R2 = 5;
  const int R3 = 6;
  
  float v_ref = 4.94; 
  float r1 = 0.00; 
  float r_ref1 = 1000.00; 
  float adc_value1 = 0.00;
  float voltage1 = 0.00; 
  float r2 = 0.00; 
  float r_ref2 = 10000.00;
  float adc_value2 = 0.00;
  float voltage2 = 0.00; 
  float r3 = 0.00; 
  float r_ref3 = 100000.00; 
  float adc_value3 = 0.00;
  float voltage3 = 0.00; 
  
  pinMode(R1,OUTPUT);
  pinMode(R2,INPUT);
  pinMode(R3,INPUT);
  digitalWrite(R1,HIGH); 
  for (int i = 0; i < 20 ; i++){ 
    adc_value1 = adc_value1 + analogRead(Ohmmeter); 
    delay(3); 
  } 

  adc_value1 = adc_value1 / 20; 

  if (adc_value1 < 1022.90){ 
    voltage1 = ((adc_value1 * v_ref) / 1024); 
    r1 = (voltage1 * r_ref1) / (v_ref - voltage1); 
  }
  
  pinMode(R1,INPUT); 
  pinMode(R2,OUTPUT); 
  pinMode(R3,INPUT); 
  digitalWrite(R2,HIGH); 
  for (int i = 0; i < 20 ; i++){ 
    adc_value2 = adc_value2 + analogRead(Ohmmeter); 
    delay(3); 
  } 
  adc_value2 = adc_value2 / 20; 
  if (adc_value2 < 1022.90){ 
    voltage2 = ((adc_value2 * v_ref) / 1024); 

    r2 = (voltage2 * r_ref2) / (v_ref - voltage2); 
  }
  
  pinMode(R1,INPUT); 
  pinMode(R2,INPUT); 
  pinMode(R3,OUTPUT); 
  digitalWrite(R3,HIGH); 
  for (int i = 0; i < 20 ; i++){ 
    adc_value3 = adc_value3 + analogRead(Ohmmeter); 
    delay(3); 
  } 
  adc_value3 = adc_value3 / 20; 
  if (adc_value3 < 1022.90){ 
    voltage3 = ((adc_value3 * v_ref) / 1024); 
    r3 = (voltage3 * r_ref3) / (v_ref - voltage2); 
  }
  
  r1 = r1 / 1000; 
  r2 = r2 / 1000; 
  r3 = r3 / 1000; 
  if (r1 < 2 && r2 < 101 && r3 < 1001) resistance = r1*1000; 
  else if (r1 > 2 && r2 < 101 && r3 < 1001) resistance = r2; 
  else if (r1 > 2 && r2 > 101 && r3 < 2000) resistance = r3; 
  else resistance = 0.00; 
  Serial.print("R = "); 
  Serial.println(resistance, 2);

  delay(500);
} 

void calculate_capacitance(){
  const int ChargePin = 2;
  const int DischargePin = 3;

  unsigned long start_time;
  unsigned long elapsed_time; 
  float microFarads; 
  float nanoFarads; 
  float r_ref = 10000.00;
  digitalWrite(ChargePin,HIGH);
  start_time = millis();
  //Serial.println(analogRead(Capacitancmeter));
  while (analogRead(Capacitancmeter) < 648) {
    Serial.println("Charging: " + String(analogRead(Capacitancmeter)) + "/648");
  }
  elapsed_time = millis() - start_time;
  microFarads = ((float)elapsed_time / r_ref) * 1000;
  if (microFarads > 1){
    capacitance = microFarads;

    Serial.print("Capacitance (uF): ");
    Serial.println(capacitance);
  
  }else{ 
    nanoFarads = microFarads * 1000.0;
    capacitance = nanoFarads;

    Serial.print("Capacitance (nF): ");
    Serial.println(capacitance);
  }
  
  digitalWrite(ChargePin,LOW);
  pinMode(DischargePin,OUTPUT);
  digitalWrite(DischargePin,LOW);
  while (analogRead(Capacitancmeter) > 0) {}
  pinMode(DischargePin,INPUT);

  delay(500);
}

void calculate_inductance(){
  const int ChargePin = 8;

  digitalWrite(ChargePin,HIGH);
  delay(5);                                               //give some time to charge inductor.
  digitalWrite(ChargePin,LOW);
  delayMicroseconds(100);                                 //make sure resination is measured
  double pulse = pulseIn(Inductancemeter,HIGH,5000);      //returns 0 if timeout
  if (pulse > 0.1){                                       //if a timeout did not occur and it took a reading:
    double capacitance = 2.E-6;                           // 2 x 1uF capacitors in parallel
    double frequency = 1.E6/(2*pulse);
    inductance = 1./(capacitance*frequency*frequency*4.*3.14159*3.14159);
    inductance *= 1000.00;                                //Comment this out to convert the output to uH
    Serial.print("inductance (mH):");
    Serial.println(inductance);
  }
}

